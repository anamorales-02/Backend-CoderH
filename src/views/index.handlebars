  <ul class="list-group">
    {{#each products}}
      <li class="list-group-item">
        <h3>{{this.title}}</h3>
        <p>ID: {{this.id}}</p>
        <p>{{this.description}}</p>
        <p>Precio: {{this.price}}</p>
